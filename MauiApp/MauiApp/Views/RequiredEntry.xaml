<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiBeyond.Views.RequiredEntry"
             x:Name="RequiredEntryView">
    <Frame x:Name="mainFrame" Margin="0,0,0,0" Padding="0,0,0,0">
        <Entry x:Name="mainEntry" Text="{Binding Text, Source={x:Reference RequiredEntryView}}" />
    </Frame>
</ContentView>
