<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiBeyond.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiBeyond"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Grid"
        ContentTemplate="{DataTemplate local:GridPage}"
        Route="GridPage" />
    <ShellContent
        Title="Stack"
        ContentTemplate="{DataTemplate local:StackPage}"
        Route="StackPage" />
</Shell>
