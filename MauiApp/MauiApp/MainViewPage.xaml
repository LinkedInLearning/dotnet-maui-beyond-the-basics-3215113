<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiBeyond.MainViewPage"
             xmlns:nameviews="clr-namespace:MauiBeyond.Views"
             xmlns:views="clr-namespace:MauiBeyond.Views;assembly=MauiBeyond.Views"
             Title="MainView Page">
    <VerticalStackLayout>
        <views:AddressView Name="{Binding Name}"></views:AddressView>
        <nameviews:NameView Name="{Binding Name}" />
    </VerticalStackLayout>
</ContentPage>
